


import matplotlib.pyplot as plt


# create data
x = [1,2,3,4,5]
y = [2,3,5,7,11]

# plot data
plt.plot(x, y)

# show plot
plt.show()





# customize appearance
plt.plot(x, y, color="green", linestyle="--", marker="o", markersize=5)

# show plot
plt.show()





# add labels and a title
plt.plot(x, y)
plt.xlabel("X-axis")
plt.ylabel("Y-axis")
plt.title("An Example Line Plot!")

# show plot
plt.show()





